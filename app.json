{
  "expo": {
    "name": "Dual-Lists App",
    "slug": "dual-lists-app",
    "description": "(React Native (.expo)) | Simple shopping list tracking app. Features include adding new items to the To Find list and found items to the Cart list.",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/main.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ak89mkii.simplelistapp"
    },
    "android": {
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/main.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [],
      "package": "com.ak89mkii.simplelistapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
